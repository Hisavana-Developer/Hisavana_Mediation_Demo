<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.mediation.ssp.ui.NativeMainActivity">

        <ScrollView
            android:id="@+id/scroll_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="2dp"
            android:background="@color/anythink_F5F5F5"
            android:padding="@dimen/anythink_padding">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <RelativeLayout
                    android:id="@+id/nativeBtn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:padding="@dimen/anythink_8dp">

                    <LinearLayout
                        android:id="@+id/ll_native"
                        android:layout_width="@dimen/anythink_pic_size"
                        android:layout_height="@dimen/anythink_pic_size"
                        android:background="@drawable/shape_gray_width1_corners2"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/iv_native"
                            android:layout_width="@dimen/anythink_pic_width"
                            android:layout_height="@dimen/anythink_pic_height"
                            android:src="@mipmap/ic_test_native" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/anythink_title_margin_left"
                        android:layout_toRightOf="@id/ll_native"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_title_native"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Native"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/anythink_title_size" />
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/nativeListBtn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/anythink_10dp"
                    android:background="@color/white"
                    android:padding="@dimen/anythink_8dp">

                    <LinearLayout
                        android:id="@+id/ll_native_express"
                        android:layout_width="@dimen/anythink_pic_size"
                        android:layout_height="@dimen/anythink_pic_size"
                        android:background="@drawable/shape_gray_width1_corners2"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/iv_native_express"
                            android:layout_width="@dimen/anythink_pic_width"
                            android:layout_height="@dimen/anythink_pic_height"
                            android:src="@mipmap/ic_test_native" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/anythink_title_margin_left"
                        android:layout_toRightOf="@id/ll_native_express"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_title_native_express"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Native List"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/anythink_title_size" />
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/native_icon_Btn"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/anythink_10dp"
                    android:background="@color/white"
                    android:padding="@dimen/anythink_8dp">

                    <LinearLayout
                        android:id="@+id/ll_nativeList_express"
                        android:layout_width="@dimen/anythink_pic_size"
                        android:layout_height="@dimen/anythink_pic_size"
                        android:background="@drawable/shape_gray_width1_corners2"
                        android:gravity="center"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/iv_nativeIcon_express"
                            android:layout_width="@dimen/anythink_pic_width"
                            android:layout_height="@dimen/anythink_pic_height"
                            android:src="@mipmap/ic_test_native" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/anythink_title_margin_left"
                        android:layout_toRightOf="@id/ll_nativeList_express"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tv_title_nativeIcon_express"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Icon List"
                            android:textColor="@android:color/black"
                            android:textSize="@dimen/anythink_title_size" />
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>

        </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>