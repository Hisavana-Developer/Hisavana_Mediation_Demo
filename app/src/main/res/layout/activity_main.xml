<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tvSDKVersion"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:minHeight="50dp" />

    <ScrollView
        android:id="@+id/scroll_view"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scrollbars="none"
        android:background="@color/anythink_F5F5F5"
        android:padding="12dp"
        app:layout_constraintBottom_toTopOf="@id/tv_agreement"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvSDKVersion">
        
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/splashBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/anythink_10dp"
                android:background="@color/white"
                android:padding="@dimen/anythink_8dp">

                <LinearLayout
                    android:id="@+id/ll_splash"
                    android:layout_width="@dimen/anythink_pic_size"
                    android:layout_height="@dimen/anythink_pic_size"
                    android:background="@drawable/shape_gray_width1_corners2"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_splash"
                        android:layout_width="@dimen/anythink_pic_width"
                        android:layout_height="@dimen/anythink_pic_height"
                        android:src="@mipmap/ic_test_splash" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/anythink_title_margin_left"
                    android:layout_toRightOf="@id/ll_splash"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_title_splash"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Splash"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/anythink_title_size" />

                    <TextView
                        android:id="@+id/tv_desc_splash"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title_splash"
                        android:layout_alignLeft="@id/tv_title_splash"
                        android:layout_marginTop="@dimen/anythink_desc_margin_top"
                        android:maxLines="3"
                        android:text="Displayed immediately after the application is launched."
                        android:textSize="@dimen/anythink_desc_size" />
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/bannerBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/anythink_10dp"
                android:background="@color/white"
                android:padding="@dimen/anythink_8dp">

                <LinearLayout
                    android:id="@+id/ll_banner"
                    android:layout_width="@dimen/anythink_pic_size"
                    android:layout_height="@dimen/anythink_pic_size"
                    android:background="@drawable/shape_gray_width1_corners2"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_banner"
                        android:layout_width="@dimen/anythink_pic_width"
                        android:layout_height="@dimen/anythink_pic_height"
                        android:src="@mipmap/ic_test_banner" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/anythink_title_margin_left"
                    android:layout_toRightOf="@id/ll_banner"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_title_banner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Banner"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/anythink_title_size" />

                    <TextView
                        android:id="@+id/tv_desc_banner"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title_banner"
                        android:layout_alignLeft="@id/tv_title_banner"
                        android:layout_marginTop="@dimen/anythink_desc_margin_top"
                        android:maxLines="3"
                        android:text="Flexible formats which could appear at the top, middle or bottom of your app."
                        android:textSize="@dimen/anythink_desc_size" />
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/interstitialBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/anythink_10dp"
                android:background="@color/white"
                android:padding="@dimen/anythink_8dp">

                <LinearLayout
                    android:id="@+id/ll_interstitial"
                    android:layout_width="@dimen/anythink_pic_size"
                    android:layout_height="@dimen/anythink_pic_size"
                    android:background="@drawable/shape_gray_width1_corners2"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_interstitial"
                        android:layout_width="@dimen/anythink_pic_width"
                        android:layout_height="@dimen/anythink_pic_height"
                        android:src="@mipmap/ic_test_interstitial" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/anythink_title_margin_left"
                    android:layout_toRightOf="@id/ll_interstitial"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_title_interstitial"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Interstitial"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/anythink_title_size" />

                    <TextView
                        android:id="@+id/tv_desc_interstitial"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title_interstitial"
                        android:layout_alignLeft="@id/tv_title_interstitial"
                        android:layout_marginTop="@dimen/anythink_desc_margin_top"
                        android:maxLines="3"
                        android:text="Include Interstitial and FullScreen.Appears at natural breaks or transition points."
                        android:textSize="@dimen/anythink_desc_size" />
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/media_videoBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:layout_marginTop="@dimen/anythink_10dp"
                android:padding="@dimen/anythink_10dp">

                <LinearLayout
                    android:id="@+id/ll_rewarded_video"
                    android:layout_width="@dimen/anythink_pic_size"
                    android:layout_height="@dimen/anythink_pic_size"
                    android:background="@drawable/shape_gray_width1_corners2"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_rewarded_video"
                        android:layout_width="@dimen/anythink_pic_width"
                        android:layout_height="@dimen/anythink_pic_height"
                        android:src="@mipmap/ic_test_rewarded_video" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/anythink_title_margin_left"
                    android:layout_toRightOf="@id/ll_rewarded_video"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_title_rewarded_video"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Video"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/anythink_title_size" />

                    <TextView
                        android:id="@+id/tv_desc_rewarded_video"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title_rewarded_video"
                        android:layout_alignLeft="@id/tv_title_rewarded_video"
                        android:layout_marginTop="@dimen/anythink_desc_margin_top"
                        android:maxLines="3"
                        android:text="Users can engage with a video ad in exchange for in-app rewards."
                        android:textSize="@dimen/anythink_desc_size" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/nativeBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/anythink_10dp"
                android:background="@color/white"
                android:padding="@dimen/anythink_8dp">

                <LinearLayout
                    android:id="@+id/ll_native"
                    android:layout_width="@dimen/anythink_pic_size"
                    android:layout_height="@dimen/anythink_pic_size"
                    android:background="@drawable/shape_gray_width1_corners2"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/iv_native"
                        android:layout_width="@dimen/anythink_pic_width"
                        android:layout_height="@dimen/anythink_pic_height"
                        android:src="@mipmap/ic_test_native" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/anythink_title_margin_left"
                    android:layout_toRightOf="@id/ll_native"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tv_title_native"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Native"
                        android:textColor="@android:color/black"
                        android:textSize="@dimen/anythink_title_size" />

                    <TextView
                        android:id="@+id/tv_desc_native"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tv_title_native"
                        android:layout_alignLeft="@id/tv_title_native"
                        android:layout_marginTop="@dimen/anythink_desc_margin_top"
                        android:maxLines="3"
                        android:text="Provide more personalized configurations"
                        android:textSize="@dimen/anythink_desc_size" />
                </LinearLayout>
            </RelativeLayout>


        </LinearLayout>

    </ScrollView>
